{
  "name": "mvrt-website",
  "private": true,
  "version": "0.0.0",
  "description": "Yeoman workflow for mvrt-website",
  "scripts": {
    "build": "./node_modules/.bin/gulp",
    "dev": "./node_modules/.bin/gulp serve",
    "preinstall": "bundle install && bower install",
    "postinstall": "./node_modules/.bin/gulp"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "waypoints": "./app/_bower_components/waypoints/lib/jquery.waypoints.js",
    "sticky": "./app/_bower_components/waypoints/lib/shortcuts/sticky.js"
  },
  "browserify-shim": {
    "waypoints": {
      "exports": "Waypoint",
      "depends": [
        "jquery:jQuery"
      ]
    },
    "sticky": {
      "depends": [
        "jquery:jQuery",
        "waypoints:Waypoint"
      ]
    }
  },
  "dependencies": {},
  "devDependencies": {
    "async": "^0.9.0",
    "browser-sync": "^1.8.2",
    "browserify": "^8.0.3",
    "browserify-shim": "^3.8.2",
    "dateformat": "^1.0.11",
    "del": "^1.1.1",
    "github-api": "^0.10.3",
    "gulp": "^3.8.10",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-filter": "^2.0.0",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.1.0",
    "gulp-jshint": "^1.9.0",
    "gulp-load-plugins": "^0.8.0",
    "gulp-minify-css": "^0.3.11",
    "gulp-minify-html": "^0.1.8",
    "gulp-rename": "^1.2.0",
    "gulp-rev": "^2.0.1",
    "gulp-rev-replace": "^0.3.1",
    "gulp-ruby-sass": "^1.0.0-alpha",
    "gulp-size": "^1.1.0",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-streamify": "0.0.5",
    "gulp-svgmin": "^1.0.0",
    "gulp-uglify": "^1.0.2",
    "gulp-useref": "^1.1.0",
    "gulp-util": "^3.0.1",
    "gulp-watch": "^3.0.0",
    "imagemin-pngquant": "^4.0.0",
    "jquery": "^2.1.3",
    "js-yaml": "^3.2.5",
    "jshint-stylish": "^1.0.0",
    "mousetrap": "^1.4.6",
    "ramda": "^0.8.0",
    "run-sequence": "^1.0.2",
    "slug": "^0.8.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^2.2.1"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
