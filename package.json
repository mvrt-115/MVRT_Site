{
  "name": "mvrt-website",
  "private": true,
  "version": "0.0.0",
  "description": "Yeoman workflow for mvrt-website",
  "scripts": {
    "build": "gulp",
    "dev": "gulp serve",
    "preinstall": "bundle install && bower install"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "waypoints": "./app/_bower_components/waypoints/lib/jquery.waypoints.js",
    "sticky": "./app/_bower_components/waypoints/lib/shortcuts/sticky.js"
  },
  "browserify-shim": {
    "waypoints": {
      "exports": "Waypoint",
      "depends": [
        "jquery:jQuery"
      ]
    },
    "sticky": {
      "depends": [
        "jquery:jQuery",
        "waypoints:Waypoint"
      ]
    }
  },
  "dependencies": {},
  "devDependencies": {
    "async": "^2.0.1",
    "bower": "^1.8.4",
    "browser-sync": "^2.24.4",
    "browserify": "^16.1.0",
    "browserify-shim": "^3.8.2",
    "concurrent-transform": "^1.0.0",
    "dateformat": "^1.0.11",
    "del": "^3.0.0",
    "github-api": "^2.3.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-filter": "^4.0.0",
    "gulp-if": "^2.0.1",
    "gulp-image-resize": "^0.13.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-jshint": "^2.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-minify-css": "^1.2.4",
    "gulp-minify-html": "^1.0.6",
    "gulp-rename": "^1.2.0",
    "gulp-rev": "^7.1.2",
    "gulp-rev-replace": "^0.4.3",
    "gulp-ruby-sass": "^2.1.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-streamify": "1.0.2",
    "gulp-svgmin": "^1.0.0",
    "gulp-uglify": "^2.0.0",
    "gulp-useref": "^3.1.5",
    "gulp-util": "^3.0.1",
    "gulp-watch": "^4.3.9",
    "imagemin-pngquant": "^5.1.0",
    "jquery": "^3.1.0",
    "js-yaml": "^3.2.5",
    "jshint": "^2.9.2",
    "jshint-stylish": "^2.2.1",
    "mousetrap": "^1.4.6",
    "ramda": "^0.22.1",
    "run-sequence": "^1.0.2",
    "to-slug-case": "^1.0.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^3.11.0"
  },
  "engines": {
    "node": ">=0.10.0"
  }
}
